{
  "root": "libs/prisma-proxy-axios-client",
  "sourceRoot": "libs/prisma-proxy-axios-client/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/prisma-proxy-axios-client",
        "main": "libs/prisma-proxy-axios-client/src/index.ts",
        "tsConfig": "libs/prisma-proxy-axios-client/tsconfig.lib.json",
        "assets": ["libs/prisma-proxy-axios-client/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/prisma-proxy-axios-client/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/prisma-proxy-axios-client"],
      "options": {
        "jestConfig": "libs/prisma-proxy-axios-client/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["client"]
}
